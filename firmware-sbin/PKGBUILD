pkgname=firmware-sbin
pkgver=1
pkgrel=1
pkgdesc="represents Telesphoreo / iPhoneOS conflicts"
arch=(any)
license=(Unlicense)
install=firmware-sbin.install

package() {
  mkdir -p $pkgdir/{sbin,usr/sbin}
  touch $pkgdir/{sbin/{fsck,fsck_apfs,fsck_exfat,fsck_hfs,fsck_msdos,launchd,mount,mount_apfs,newfs_apfs,newfs_hfs,pfctl,umount},usr/sbin/{BTAvrcp,BTLEServer,BTMap,BTPbap,BlueTool,WirelessRadioManagerd,absd,addNetworkInterface,aslmanager,bluetoothd,cfprefsd,distnoted,filecoordinationd,ioreg,ipconfig,mDNSResponder,mDNSResponderHelper,mediaserverd,notifyd,nvram,pppd,racoon,rtadvd,scutil,spindump,syslogd,wifid}}
}
